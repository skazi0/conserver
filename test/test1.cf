# test conserver config file
default full {
	rw *;
}
default * {
	logfile ./&;
	timestamp "";
	include full;
}
config * {
	reinitcheck 1s;
}
break 5 {
	string "\rtest\r";
}
console shell {
	master 127.0.0.1;
	logfile ./&.log;
	timestamp 5;
	type exec;
	exec "";
}
console shell2 {
	master 127.0.0.1;
	logfile ./shell2.log;
	timestamp 2;
	type exec;
	exec "";
}
access * {
	trusted 127.0.0.1;
}
